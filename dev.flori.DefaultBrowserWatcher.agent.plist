<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>dev.flori.DefaultBrowserWatcher</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/default-browser-watcher</string>
    </array>
    <key>KeepAlive</key>
    <dict>
        <key>Crashed</key>
        <true/>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>EnvironmentVariables</key>
    <dict>
        <key>BROWSER_WATCHER_BACKEND_API_TOKEN</key>
        <string>your_api_token</string>
    </dict>
</dict>
</plist>
